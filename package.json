{
  "name": "generator-angular1-gulp",
  "version": "0.1",
  "repository": "rkori215/generator-angular1-gulp",
  "description": "Yeoman Fountain generator to scaffold a webapp with Angular 1 written in ES6 (Babel), TypeScript through Webpack or SystemJS including tools Gulp 4, ESLint, Browsersync and Karma",
  "main": "index.js",
  "keywords": [
    "yeoman-generator",
    "yeoman",
    "generator",
    "gulp",
    "gulpfile",
    "fountain",
    "cli",
    "angular"
  ],
  "dependencies": {
    "fountain-generator": "^1.0.0-rc1",
    "generator-fountain-gulp": "^1.0.0-rc1"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-eslint": "^6.0.2",
    "chai": "^3.4.1",
    "chai-spies": "^0.7.1",
    "cross-spawn": "^4.0.0",
    "eslint": "^3.2.2",
    "eslint-config-xo-space": "^0.12.0",
    "eslint-plugin-babel": "^3.2.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-eslint": "^2.0.0",
    "gulp-exclude-gitignore": "^1.0.0",
    "gulp-nsp": "^2.4.0",
    "nyc": "^6.6.1"
  },
  "nyc": {
    "include": [
      "generators/**/*.js"
    ],
    "exclude": [
      "generators/**/templates/**"
    ]
  },
  "scripts": {
    "test": "gulp",
    "prepublish": "gulp prepublish"
  },
  "license": "MIT",
  "eslintConfig": {
    "extends": "xo-space/esnext"
  }
}
